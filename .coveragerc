[run]

branch = true

data_file = .cache/coverage

omit =
    .venv/*
    */tests/*
    */__main__.py

[report]

exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
